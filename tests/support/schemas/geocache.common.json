{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "geocache": {
      "type": "object",
      "required": [
        "id",
        "location"
      ],
      "properties": {
        "id": {
          "type": "integer"
        },
        "location": {
          "type": "object",
          "required": [
            "lat",
            "lng",
            "geohash"
          ],
          "properties": {
            "lat": {
              "type": "number"
            },
            "lng": {
              "type": "number"
            },
            "geohash": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}